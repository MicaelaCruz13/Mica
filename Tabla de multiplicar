// 11) Tabla de multiplicar
void tablaMultiplicar() {
    int numero;

    printf("Ingrese un n√∫mero para ver su tabla de multiplicar: ");
    scanf("%d", &numero);

    printf("Tabla de multiplicar del %d:\n", numero);
    for (int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }
}
